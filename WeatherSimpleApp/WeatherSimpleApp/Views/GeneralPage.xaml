             xmlns:d="http://xamarin.com/schemas/2014/forms/design"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             x:Class="WeatherSimpleApp.GeneralPage"
             xmlns:pages="clr-namespace:WeatherSimpleApp"
             Visual="Material"
             >
  <MasterDetailPage.Master>
        <pages:GeneralPageMaster x:Name="MasterPage" />
    </MasterDetailPage.Master>
  <MasterDetailPage.Detail>
    <NavigationPage>
      <x:Arguments>
            <pages:GeberalTabbed />
      </x:Arguments>
    </NavigationPage>
  </MasterDetailPage.Detail>
</MasterDetailPage>